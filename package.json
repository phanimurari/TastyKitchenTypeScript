    "mock-local-storage": "1.1.17",
    "msw": "0.34.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-i18next": "11.15.5",
    "react-icons": "4.3.1",
    "react-loader-spinner": "4.0.0",
    "react-router-dom": "5.2.0",
    "react-slick": "0.28.1",
    "reactjs-popup": "2.0.5",
    "slick-carousel": "1.8.1",
    "styled-components": "5.3.3",
    "twin.macro": "2.8.2",
