  "homepage": "http://phanimurari.github.io/TastyKitchenTypeScript",
  "engines": {
    "node": "^10.13 || 12 || 14 || 15",
    "npm": ">=6"
  },
  "dependencies": {
    "@ib/api": "2.0.3",
    "@ib/api-constants": "2.0.0",
    "@ib/mobx-promise": "2.0.0",
